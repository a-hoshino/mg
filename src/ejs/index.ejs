<% var title = "top" -%>
<!DOCTYPE html>
<html dir="ltr" lang="ja">
<%- include('./_include/_head', {pageTitle:title}) %>
<body>
<main>
    <div class="mg-container">
        <h1 class="mg-text-xl"><%= jsonData.sitename %></h1>
        <h2 class="mg-text-s"><%= jsonData.description %></h2>
    </div>
    <ul class="mg-list">
<% jsonData.pages.forEach(function (value, key) { %>
        <li>
            <a class="mg-text-m" href="./works/<%= [value] -%>.html"><%= value %></a>
        </li>
<% }); %>
    </ul>
</main>
<script src="./js/<%= title -%>.js"></script>
</body>
</html>